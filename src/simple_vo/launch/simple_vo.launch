<launch>
    <arg name="bag_name" value="$(find simple_vo)/bags/point_grey.bag"/>
    <node pkg="rosbag" type="play" name="rosbag" args="-l $(arg bag_name)"/>

    <arg name="config_file_name" value="$(find simple_vo)/config/point_grey.yaml"/>
    <node pkg="simple_vo" type="simple_vo_node" name="simple_vo_node" output="screen">
        <param name="config_file_name" value="$(arg config_file_name)"/>
    </node>
</launch>